# LLM Configuration
ACTIVE_PROVIDER=google
ACTIVE_MODEL=gemini-pro

# Google Gemini (required for gemini provider)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI (optional)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Internal API Secret for secure communication between Next.js and Python serverless functions
# This secret is used to authenticate requests from the Next.js API route to the Python serverless function
# Generate a strong random secret and set it in both Next.js and Python function environments
INTERNAL_API_SECRET=your_secure_random_secret_here
